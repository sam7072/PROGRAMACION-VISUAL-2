@using modelo.entidades
@model Evaluacion


@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Actualizar </h1>

<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <h3>
                actualizar evaluCION
            </h3>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-10">
            <form asp-action="Edit" method="post">

                <input type="text" asp-for="EvaluacionId" hidden />


                <div class="form-group">
                    <label class="custom-control-label">Fecha: </label>
                    <input class="form-control" type="datetime-local" asp-for="FechaE" />
                </div>
                <div class="form-group">
                    <label class="custom-control-label">Calificación: </label>
                    <select class="form-control"
                            asp-for="Calificaion" asp-items="Html.GetEnumSelectList<Calificaion>()">
                    </select>
                </div>
                <div class="form-group">
                    <label class="custom-control-label">Libro: </label>
                    <select class="form-control"
                            asp-for="PresupuestoId" asp-items="@ViewBag.selectListPresupuestos">
                        <option value="">--Seleccione un empleado---</option>
                    </select>
                </div>


                <div class="form-group">
                    <input class="btn btn-primary" type="submit" value="Actualizar " />
                </div>
            </form>
        </div>
    </div>

</div>