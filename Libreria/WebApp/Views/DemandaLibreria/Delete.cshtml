@using modelo.entidades
@model DemandaLibreria

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Eliminar </h1>

<div class="container">
   <div class="row">
       <div class="col-sm-12">
           <h3>
               Eliminar:
           </h3>
       </div>
   </div>
   <div class="row">
       <div class="col-sm-10"> 
           <form asp-action="Delete" method="post">
               <input type="text" asp-for="DemandalibreriaId" hidden />


               <div class="form-group">
                   <label class="custom-control-label">Usuario: </label>
                  <input class="form-control" type="text" asp-for="Usuario" disabled/>
               </div>
               <div class="form-group">
                   <label class="custom-control-label">Libro: </label>
                  <input class="form-control" type="text" asp-for="Libro" disabled/>
               </div>
               <div class="form-group">
                   <label class="custom-control-label">Fecha: </label>
                  <input class="form-control" type="date" asp-for="Fechas" disabled/>
               </div>


               <div class="form-group">
                   @*<label class="custom-control-label">Presupuesto: </label>*@
                   <input type="text" asp-for="PresupuestoId" hidden />
                </div>
                <div class="form-group">
                    <input class="btn btn-primary" type="submit" value="Eliminar" />
                </div>



          </form>
       </div>
</div>
</div>




