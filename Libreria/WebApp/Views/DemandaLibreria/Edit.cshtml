@using modelo.entidades
@model DemandaLibreria

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Editar-Actualizar </h1>

<div class="container">
   <div class="row">
       <div class="col-sm-12">
           <h3>
               EDITAR:
           </h3>
       </div>
   </div>
   <div class="row">
       <div class="col-sm-10"> 
           <form asp-action="Edit" method="post">
               <input type="text" asp-for="DemandalibreriaId" hidden />


               <div class="form-group">
                   <label class="custom-control-label">Usuario: </label>
                  <input class="form-control" type="text" asp-for="Usuario" />
               </div>
               <div class="form-group">
                   <label class="custom-control-label">Libro: </label>
                  <input class="form-control" type="text" asp-for="Libro" />
               </div>
               <div class="form-group">
                   <label class="custom-control-label">Fecha: </label>
                  <input class="form-control" type="date" asp-for="Fechas" />
               </div>
               <div class="form-group">
                   <label class="custom-control-label">Confirmacion: </label>
                  <input class="form-control" type="text" asp-for="ConfirmacionDL" />
               </div>


               <div class="form-group">
                   <label class="custom-control-label">Presupuesto: </label>
                   <input type="text" asp-for="PresupuestoId" hidden />
                </div>
                <div class="form-group">
                    <input class="btn btn-primary" type="submit" value="Actualizar" />
                </div>



          </form>
       </div>
</div>
</div>


