@using modelo.entidades
@model Presupuesto

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Eliminar del Presupuesto</h1>

<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <h3>
                Confirmar eliminacion Presupuesto: @Model.LibrosP
            </h3>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-10">
            <form asp-action="Delete" method="post">
                <input type="text" asp-for="PresupuestoId" hidden />

                <div class="form-group">
                    <label class="custom-control-label">Nombre: </label>
                    <input class="form-control" type="text" asp-for="LibrosP" disabled/>
                </div>
                <div class="form-group">
                    <label class="custom-control-label">Cédula: </label>
                    <input class="form-control"  type="text" asp-for="PrecioP" disabled/>
                </div>
                <div class="form-group">
                    <label class="custom-control-label">Correo: </label>
                    <input class="form-control" type="text" asp-for="CantidadP" disabled/>
                </div>
                <div class="form-group">
                    <label class="custom-control-label">Celular: </label>
                    <input class="form-control"  type="text" asp-for="DistribuidorP" disabled/>
                </div>
                <div class="form-group">
                    <input class="btn btn-primary" type="submit" value="Eliminar Presupuesto" />
                </div>
            </form>
        </div>
    </div>

</div>

