@using modelo.entidades
@model Presupuesto


@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Creacion del Presupuesto</h1>

<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <h3>
                Nuevo Presupuesto
            </h3>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-10">
            <form asp-action="Create" method="post">
                <div class="form-group">
                    <label class="custom-control-label">Nombre Libro: </label>
                    <input class="form-control" type="text" asp-for="LibrosP" />
                </div>
                <div class="form-group">
                    <label class="custom-control-label">Precio </label>
                    <input class="form-control"  type="text" asp-for="PrecioP" />
                </div>
                <div class="form-group">
                    <label class="custom-control-label">cantidad </label>
                    <input class="form-control" type="text" asp-for="CantidadP" />
                </div>
                <div class="form-group">
                    <label class="custom-control-label">Distribuidor </label>
                    <input class="form-control"  type="text" asp-for="DistribuidorP" />
                </div>
                <div class="form-group">
                    <input class="btn btn-primary" type="submit" value="Crear Presupuesto" />
                </div>
            </form>
        </div>
    </div>

</div>

