@using modelo.entidades;
@model IEnumerable<Presupuesto>



@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Lista de Presupuesto</h1>
<p> Presupuesto</p>

<div class="container" >
    <div class="row">
        <div class="col-sm-5">
            <h3>Presupuesto</h3>
        </div>
        <div class="col-sm-5">
            <a href="" class="btn btn-success d-block">Agregar nuevo Presupuesto</a>
        </div>
    </div>

    <div class="row">

        <table class="table table-bordered">
            <thead>
                <tr>
                    <td>Id</td>
                    <td>Libros</td>
                    <td>Precio</td>
                    <td>Cantidad</td>
                    <td>Distribuidor</td>

                </tr>
            </thead>
            <tbody>
                @foreach (Presupuesto presupuesto in Model)
                {
                    <tr>
                        <td>@presupuesto.PresupuestoId</td>
                        <td>@presupuesto.LibrosP</td>
                        <td>@presupuesto.PrecioP</td>
                        <td>@presupuesto.CantidadP</td>
                        <td>@presupuesto.DistribuidorP</td>
                        <td>
                            <a href="#" class="btn btn-primary">Editar</a>
                            <a href="#" class="btn btn-danger">Borrar</a>
                        </td>
                    </tr>
                    
                }


            </tbody>

        </table>

    </div>








</div>